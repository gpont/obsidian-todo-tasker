{
	"name": "obsidian-todo-tasker",
	"version": "0.0.1",
	"description": "Плагин для Obsidian, который помогает управлять задачами и отслеживать их прогресс",
	"main": "main.js",
	"scripts": {
		"dev": "node build/esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node build/esbuild.config.mjs production",
		"version": "node build/version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"todo",
		"task",
		"productivity"
	],
	"author": "gpont",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
